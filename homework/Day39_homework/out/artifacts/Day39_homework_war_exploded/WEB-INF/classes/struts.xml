<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

<struts>
   <package name="user" extends="struts-default">
       <action name="login" class="com.lanou3g.user.web.UserAction" method="login">
           <result name="success">jsps/main.jsp</result>
           <result name="error">jsps/user/login.jsp</result>
       </action>

       <action name="regist" class="com.lanou3g.user.web.UserAction" method="regist">
           <result name="success">jsps/msg.jsp</result>
           <result name="error">jsps/user/regist.jsp</result>
       </action>
   </package>

</struts>